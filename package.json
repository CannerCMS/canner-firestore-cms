{
  "name": "canner-firebase-cms",
  "version": "1.0.0",
  "description": "Canner CMS for Firebase",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --history-api-fallback",
    "build": "webpack --mode production",
    "prettier": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "import:auth": "firebase auth:import firebase-default-user.json",
    "serve": "firebase serve",
    "deploy": "firebase deploy --only hosting"
  },
  "theme": {
    "menu-dark-bg": "#373d62",
    "layout-sider-background": "#373d62",
    "layout-header-background": "#373d62",
    "primary-color": "#f2b173",
    "primary-3": "rgba(242, 177, 115, 0.5)",
    "primary-4": "rgba(242, 177, 115, 0.7)",
    "menu-dark-item-selected-bg": "#283050",
    "menu-dark-item-active-bg": "#283050",
    "item-active-bg": "rgba(242, 177, 115, 0.1)",
    "item-hover-bg": "rgba(242, 177, 115, 0.3)",
    "font-size-base": "14px"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "canner-schema-loader": "3.2.1",
    "canner-script": "3.2.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "@canner/antd-components": "^0.5.29",
    "@canner/container": "^3.4.3",
    "@canner/history-router": "^3.2.1",
    "@canner/storage": "^3.2.1",
    "antd": "3.15.2",
    "canner": "^3.4.3",
    "canner-graphql-interface": "^2.0.52",
    "canner-helpers": "^3.2.1",
    "canner-layouts": "^3.2.1",
    "firebase": "^5.9.2",
    "graphql": "^14.2.0",
    "query-string": "^6.4.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-github-corner": "^2.3.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "slug": "^1.0.0",
    "styled-components": "^4.2.0"
  }
}
